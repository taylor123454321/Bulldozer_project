clear,clc, close all

load('angle_results/angle_data_7')

for i = 1:length(angles(1,3,:))
    a(i) = angles(1,1,i);
    b(i) = angles(1,2,i);
    c(i) = angles(1,3,i);
    if i > 60
        stepp(i) = 6.85;
    else
        stepp(i) = 1.8;
    end
end

figure
plot(a)
hold on
% plot(b)
% plot(c)
plot(stepp)
grid on
title('Step, buffer size 3')
xlabel('Frames')
ylabel('Rotation (degrees)')
legend('Output','Step input')






